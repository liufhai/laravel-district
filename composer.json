{
    "name": "liufhai/laravel-district",
    "description": "District Explorer For Laravel with AutoNavi. 中国行政区划数据，访问高德在线数据获取最新区划数据。",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3@dev",
        "overtrue/laravel-pinyin": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1@dev|7.5.x-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "liufhai",
            "email": "liufhai@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Liufhai\\DistrictExplorer\\ServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Liufhai\\DistrictExplorer\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
